function doZoom(size){
if(size==12){
xd("heightauto").style.fontSize = size + "px";
xd("fs12").style.display = "";
xd("fs14").style.display = "none";
xd("fs16").style.display = "none";
}
if(size==14){
xd("heightauto").style.fontSize = size + "px";
xd("fs12").style.display = "none";
xd("fs14").style.display = "";
xd("fs16").style.display = "none";
}
if(size==16){
xd("heightauto").style.fontSize = size + "px";
xd("fs12").style.display = "none";
xd("fs14").style.display = "none";
xd("fs16").style.display = "";
}
}
</SCRIPT>
